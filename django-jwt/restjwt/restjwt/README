curl -X POST -H "Content-Type: application/json" -d '{"username": "<username>", "password": "<password>"}' http://localhost:8000/api/token/

curl -X POST -H "Content-Type: application/json" -d '{"refresh": "<token>"}' http://localhost:8000/api/token/refresh/

curl -H "Authorization: Bearer <token>" http://localhost:8000/products/
